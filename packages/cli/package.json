{
  "name": "wile",
  "version": "0.6.3",
  "description": "Autonomous AI coding agent that ships features while you sleep",
  "type": "module",
  "bin": {
    "wile": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node",
