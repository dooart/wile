    "coding",
    "autonomous",
    "agent",
    "claude"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "8.57.0",
    "typescript": "^5.9.3"
  }
}
